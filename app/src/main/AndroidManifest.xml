<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityOne" />
        <activity android:name=".ActivityTwo" android:label="@string/profile" android:theme="@style/AppTheme.GreenNavigationBar"/>
        <activity android:name=".ActivityThree" android:label="@string/settings" android:theme="@style/AppTheme.GreenNavigationBar"/>
        <activity android:name=".ActivityFour" android:label="@string/feedback" android:theme="@style/AppTheme.GreenNavigationBar"/>
    </application>

</manifest>